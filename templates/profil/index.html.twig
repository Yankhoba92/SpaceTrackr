{% extends 'base.html.twig' %}

{% block title %}
	{{ app.user.username }} | Profil
{% endblock %}

{% block body %}
		{# card bootstrap profil #}
		<div class="container">
			<div class="card">
				<div class="card-body">
					<h5 class="card-title">{{ app.user.username }}</h5>
					<p class="card-text">{{ app.user.email }}</p>
					<p class="visually-hidden"> {{ app.user.password }} </p>
				</div>
			</div>
		</div>
{% endblock %}